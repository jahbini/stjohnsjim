  bloviation: =>
    T.div "#bloviation.article-entry.contents", "dangerously-set-inner-h-t-m-l": "[object Object]", =>
      T.p =>
        T.raw "The word on the street is that there is a huge marijuana grow over at Pier Park, St. John’s, Portland, OR, USA, land of the brave or the free ("
        T.em => T.raw "bravery not related to freedom, offer extended only until 2033"
        T.raw ")."
      T.p =>
        T.raw "We Are Shocked! Marijuana is a"
        T.a href: "http://www.huffingtonpost.com/2011/09/21/los-angeles-crime-rise-pot-clinics_n_973632.html", => T.raw "menace to society"
        T.raw "that is akin to"
        T.a href: "http://www.google.com/search?rls=en&q=pornography+crime+statistics&ie=UTF-8&oe=UTF-8", => T.raw "pornography"
