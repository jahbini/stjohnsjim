  post_a_week_of_false_starts: =>
    T.div "#post-a-week-of-false-starts.article.article-type-post", itemscope: "itemscope", itemprop: "blogPost", =>
      T.div ".article-inner.pb2", =>
        T.div ".article-header", =>
