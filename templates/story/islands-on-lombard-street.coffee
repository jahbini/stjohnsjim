  post_islands_on_lombard_street: =>
    T.div "#post-islands-on-lombard-street.article.article-type-post", itemscope: "itemscope", itemprop: "blogPost", =>
      T.div ".article-inner.pb2", =>
        T.div ".article-header", =>
