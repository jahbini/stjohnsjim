  main: =>
    T.div "#main", id: "main", =>
      @post_thanksgiving()
  # 
  # section post_thanksgiving
  # 
  post_thanksgiving: =>
    T.div "#post-thanksgiving.article.article-type-post", id: "post-thanksgiving", class: "article article-type-post", itemscope: "itemscope", itemprop: "blogPost", =>
      T.div ".article-inner.pb2", class: "article-inner pb2", =>
        T.div ".article-header", class: "article-header", =>
          T.h1 ".article-title", class: "article-title", itemprop: "name", => T.raw "Thanksgiving"
          @bloviation()
        T.div ".article-footer.hide", class: "article-footer hide", =>
