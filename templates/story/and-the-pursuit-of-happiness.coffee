  post_and_the_pursuit_of_happiness: =>
    T.div "#post-and-the-pursuit-of-happiness.article.article-type-post", itemscope: "itemscope", itemprop: "blogPost", =>
      T.div ".article-inner.pb2", =>
        T.div ".article-header", =>
