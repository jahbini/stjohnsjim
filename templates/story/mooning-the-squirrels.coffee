  post_mooning_the_squirrels: =>
    T.div "#post-mooning-the-squirrels.article.article-type-post", itemscope: "itemscope", itemprop: "blogPost", =>
      T.div ".article-inner.pb2", =>
        T.div ".article-header", =>
