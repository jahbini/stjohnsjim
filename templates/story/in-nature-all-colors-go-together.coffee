  post_in_nature_all_colors_go_together: =>
    T.div "#post-in-nature-all-colors-go-together.article.article-type-post", itemscope: "itemscope", itemprop: "blogPost", =>
      T.div ".article-inner.pb2", =>
        T.div ".article-header", =>
